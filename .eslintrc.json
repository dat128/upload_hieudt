{
    "extends": ["airbnb-base", "prettier"],
    "plugins": ["prettier"],
    "rules": {
        "camelcase": "off",
        "prettier/prettier": ["error", {
            "printWidth": 120,
            "trailingComma": "es5",
            "semi": true,
            "jsxSingleQuote": true,
            "singleQuote": true,
            "useTabs": true,
            "endOfLine": "auto",
            "camelCase": "off"
        }]
    }
}